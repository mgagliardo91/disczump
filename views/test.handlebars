<!doctype html>
<html>
<head>
    <title>DiscZump - Sign In</title>
    <link rel="stylesheet" href="/static/css/test.css">
    <link rel="stylesheet" href="/static/css/jquery-ui.min.css">
</head>
<body>
    
    
    
    <div class="row">
        <div class="col-md-6 col-md-offset-3">
            <div class="disc-inventory-header" id="inventory-header">
                <div class="header-static header-row">
                    <div class="div-inline float-left" style="width: 30%">
                        <form class="form-inline" role="form">
                            <div class="form-group">
                                <p class="form-control-static header-text">Display:</p>
                                <select class="form-control input-sm" id="paginate-display-count">
                                    <option value="20">20</option>
                                    <option value="40">40</option>
                                    <option value="60">60</option>
                                    <option value="80">80</option>
                                    <option value="100">100</option>
                                    <option value="All">All</option>
                                </select>
                            </div>
                        </form>
                    </div>
                    <div class="div-inline float-left text-center" style="width: 40%">
                        <ul class="list-inline form-control-static" id="paginate-nav">
                            <li class="page-static" id="page-back"><span class="glyphicon glyphicon-chevron-left"></span></li>
                            <li class="page-static" id="page-forward"><span class="glyphicon glyphicon-chevron-right"></span></li>
                        </ul>
                    </div>
                    <div class="div-inline float-left" style="width: 30%">
                        <div class="div-inline float-left header-text text-left" style="width: 40%" id="results-header-sort">
                            <i class="fa fa-sort"></i> Sort
                        </div>
                        <div class="div-inline float-left header-text text-right" style="width: 60%" id="results-header-count">
                            Results: 0
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="header-dynamic header-row" id="disc-inventory-header-dynamic">
                    <div class="header-sort-container">
                        <div class="current-sort-container">
                        </div>
                        <div class="add-sort-container no-select">
                            <div class="text-center"><span><i class="fa fa-plus-circle"></i></span> Add Field</div>
                        </div>
                    </div>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="disc-inventory-container">
                <div class="disc-inventory-body">
                    <div class="disc-item-container">
                        <div class="disc-item">
                            <div class="disc-content-image-container">
                                <div class="disc-content-image">
                                    <img src="https://placehold.it/150x150" />
                                </div>
                            </div>
            
                            <div class="disc-content-info-container">
                                <div class="disc-info-main-pane">
                                    <div class="disc-info-left-pane div-inline float-left">
                                        <div class="disc-info-brand">Discraft</div>
                                        <div class="disc-info-name">Zone</div>
                                    </div>
                                    <div class="disc-info-right-pane div-inline float-left">
                                        <div class="div-inline float-left div-split-horiz">
                                            <table>
                                                <tr>
                                                    <td class="disc-info-label">Type:</td>
                                                    <td class="disc-info-value">Putter</td>
                                                </tr>
                                                <tr>
                                                    <td class="disc-info-label">Material:</td>
                                                    <td class="disc-info-value">Plastic</td>
                                                </tr>
                                            </table>
                                        </div>
                                        <div class="div-inline float-left div-split-horiz">
                                            <table>
                                                <tr>
                                                    <td class="disc-info-label">Color:</td>
                                                    <td class="disc-info-value">Blue</td>
                                                </tr>
                                                <tr>
                                                    <td class="disc-info-label">Weight:</td>
                                                    <td class="disc-info-value">172g</td>
                                                </tr>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div>
                                    <div class="disc-info-left-pane div-inline float-left">
                                        <div class="disc-info-bottom">
                                            <div class="disc-info-numbers">10 | 0 | -1 | 10</div>
                                        </div>
                                    </div>
                                    <div class="disc-info-right-pane div-inline float-left">
                                        <div class="disc-info-bottom">
                                            <div class="disc-info-tags div-inline float-left">
                                                <span class="disc-info-tag-label">Tags:</span>
                                                <span class="disc-info-tag">Halloween</span>
                                                <span class="disc-info-tag">Halloween</span>
                                                <span class="disc-info-tag">Halloween</span>
                                                <span class="disc-info-tag">Halloween</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <br />
    <br />
    <br />
    <br />
    <br />
    <div class="row">
        <div class="col-md-4 col-md-offset-3">
            <form class="form-horizontal" role="form" id="createDiscForm" autocomplete="off">
                        <div class="form-group">
        	                  <label class="col-sm-2 control-label">Tags</label>
        	                  <div class="col-sm-10">
        	                      <div class="tag-input-group">
        	                          <div class="input-group">
        	                            <input type="text" class="form-control add-disc-tag">
        	                            <div class="input-group-addon"><span><i class="fa fa-angle-double-down"></i></span></div>
        	                          </div>
        	                           <div class="tag-list-display">
                                            <ul class="list-unstyled tag-search-list">
                                                <!--<li class="tag-list-item">
                                                    <span><i class="fa fa-tag"></i></span>
                                                    January
                                                </li>-->
                                            </ul>
                                        </div>
                                        <div class="tag-list-container">
        	                              <!--<div class="tag-item">
        	                                <p class="tag-item-text">Halloween <span><i class="fa fa-times"></i></span></p>
        	                              </div>-->
        	                          </div>
        	                      </div>
        	                  </div>
        	              </div>
        	    <button type="button" id="btn-add-tag" class="btn btn-primary">Go</button>
            </form>
        </div>
        <div class="col-md-1 col-md-offset-8">
        </div>
    </div>
    
    
    <div class="login-form-container">
        <div class="login-form-cell">
            <div class="login-form-center">
                <form action="/login" method="post">
                    <h1 class="login-title">Test</h1>
                    <div class="login-form-stacked">
                        <input id="fileguy" type="file" />
                    </div>
                    <div class="form-header"><a href="/recover">Forgot Password?</a></div>
                </form>
                <p id="output">Output</p>
                <div class="upload-img" id="disc-img-front" style="width:100px;height:300px">
                </div>
                <button id="submit" class="btn btn-submit btn-block">Sign in</button>
            </div>
        </div>
    </div>
 
    
    <div class="row-fluid">
        <div class="col-md-6 col-md-offset-3">
            <div class="image-block-display">
                <div class="image-block">
                    <div class="image-block-frame">
                        <img src="https://placehold.it/350x350" class="fit-parent">
                    </div>
                </div>
                <div class="image-list">
                    <div class="image-list-container image-list-container-simple">
                        <div class="image-list-table">
                            <div class="image-item-container">
                                <div class="image-item">
                                    <div class="image-preview">
                                        <img src="https://placehold.it/350x150" class="fit-parent">
                                    </div>
                                </div>
                            </div>
                            <div class="image-item-container">
                                <div class="image-item">
                                    <div class="image-preview">
                                        <img src="https://placehold.it/350x350" class="fit-parent">
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    
    
    
    
    <script src="https://rawgit.com/enyo/dropzone/master/downloads/dropzone.js"></script>
    <script src="/static/js/jquery-ui.min.js"></script>
    <script src="/static/js/test.js"></script>
</body>
</html>
